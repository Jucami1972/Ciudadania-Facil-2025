# Configuración de npm para manejar peer dependencies
# Esto permite que npm instale paquetes aunque haya advertencias de peer dependencies
legacy-peer-deps=false

# Suprimir warnings de peer dependencies (no afecta la funcionalidad)
fund=false
audit=false

# Permitir override de peer dependencies para React 19
# (los warnings son normales cuando se usa React 19 con librerías que aún no lo soportan oficialmente)
